{
	"name": "announcerbot",
	"version": "1.0.0",
	"description": "The bot for ACG (RBLX Sci-fi community)",
	"main": "dist/index.js",
	"dependencies": {
		"discord.js": "^13.6.0",
		"dotenv": "^16.0.0"
	},
	"devDependencies": {
		"@types/node": "^17.0.31",
		"@typescript-eslint/eslint-plugin": "^5.22.0",
		"@typescript-eslint/parser": "^5.22.0",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "^2.6.2",
		"typescript": "^4.6.4"
	},
	"scripts": {
		"lint": "eslint src",
		"lint-fix": "eslint --fix src",
		"compile": "tsc",
		"build": "npm run lint && npm run compile",
		"start": "node -r dotenv/config .",
		"dev": "npm run compile && node -r dotenv/config --inspect .",
		"pm2": "pm2 startOrRestart pm2.config.json --trace --update-env"
	},
	"author": "Mokiros",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/WaviestBalloon/AnnouncerBot.git"
	},
	"bugs": {
		"url": "https://github.com/WaviestBalloon/AnnouncerBot/issues"
	},
	"homepage": "https://github.com/WaviestBalloon/AnnouncerBot#readme"
}
